all: FYR.COM MICED.COM PASSWORD.COM SNOW.COM STAR.COM BRICK.COM

FYR.COM: FYR.ASM
	MSA2 FYR.ASM -o FYR.COM

MICED.COM: MICED.ASM
	MSA2 MICED.ASM -o MICED.COM

PASSWORD.COM: PASSWORD.ASM
	MSA2 PASSWORD.ASM -o PASSWORD.COM

SNOW.COM: SNOW.ASM
	MSA2 SNOW.ASM -o SNOW.COM

STAR.COM: STAR.ASM
	MSA2 STAR.ASM -o STAR.EXE -f texe

BRICK.COM: BRICK.ASM
	MSA2 BRICK.ASM -o BRICK.COM

clean:
	DEL *.COM
	DEL *.EXE

prof:
	DEL *.SMP
	WSAMPLE MSA2.EXE MICED.ASM -o MICED.COM
	COPY MSA2.SMP MICED.SMP
	WSAMPLE MSA2.EXE SNOW.ASM -o SNOW.COM
	COPY MSA2.SMP SNOW.SMP
	DEL MSA2.SMP
