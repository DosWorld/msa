all: FYR.COM MICED.COM PASSWORD.COM SNOW.COM STAR.COM BRICK.COM EXESAMPL.EXE

FYR.COM: FYR.ASM
   MSA2 FYR.ASM -oFYR.COM

MICED.COM: MICED.ASM
   MSA2 MICED.ASM -oMICED.COM

PASSWORD.COM: PASSWORD.ASM
   MSA2 PASSWORD.ASM -oPASSWORD.COM

SNOW.COM: SNOW.ASM
   MSA2 SNOW.ASM -oSNOW.COM

STAR.COM: STAR.ASM
   MSA2 STAR.ASM -oSTAR.EXE -ftexe

BRICK.COM: BRICK.ASM
   MSA2 BRICK.ASM -oBRICK.COM

EXESAMPL.EXE: EXESAMPL.ASM
   MSA2 EXESAMPL.ASM -oEXESAMPL.EXE -ftexe

clean:
   DEL *.COM
   DEL *.EXE

