all: msa2

distcheck:


checks:

msa2: MSA.C TABLES.C ASSEMBLR.C MISC.C LEX.C MSA2.H LEX.H
	gcc -O3 MSA.C TABLES.C ASSEMBLR.C MISC.C LEX.C -o msa2

msa2w.exe: MSA.C TABLES.C ASSEMBLR.C MISC.C LEX.C MSA2.H LEX.H
	i686-w64-mingw32-gcc -m32 -O3 MSA.C TABLES.C ASSEMBLR.C MISC.C LEX.C -o MSA2W.EXE
	strip MSA2W.EXE

dos: msa2.exe

msa2.exe: MSA.C TABLES.C ASSEMBLR.C MISC.C LEX.C MSA2.H LEX.H
	wcl MSA.C TABLES.C ASSEMBLR.C LEX.C MISC.C -fe=msa2.exe -ms -ox -0
	rem upx -9 --8086 msa2.exe

clean:
	del msa2.exe
	del *.obj
	del *.err
	del *.ori

install:
	copy MSA2.EXE ..\BIN\MSA2.EXE
	copy MSA2W.EXE ..\BINW\MSA2W.EXE
